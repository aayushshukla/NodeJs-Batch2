<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <div><%- include('partials/header') %></div>
    <table border="1">
      <tr>
        <th>Employee Id</th>
        <th>Employee Name</th>
        <th>Employee Email</th>
        <th>Employee Password</th>
      </tr>

      <% allEmpDetails[0].forEach(emp => { %>

      <tr>
        <td><%= emp.eid %></td>
        <td><%= emp.ename%></td>
        <td><%= emp.email %></td>
        <td><%= emp.epass %></td>
        <td><a href="">Update</a></td>
        <td><a href="/web/delete?eid=<%=emp.eid%>">Delete </a></td>
      </tr>

      <% }); %>
    </table>

    <div><%- include('partials/footer') %></div>
  </body>
</html>
